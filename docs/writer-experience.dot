  x [label='Knowledgebase', fillcolor=green]
  y [label='Review team', fillcolor=green]
  z [label='Issues Source', fillcolor=orange]
  
 
  z -> a  [label='&nbsp;Issues Info', weight=3, fontsize=20, dir=both];
